<Project>

  <PropertyGroup>
    <Company>PagoPA Spa</Company>
    <Authors>Team DevEx</Authors>
    <Copyright>Copyright Â©PagoPA $([System.DateTime]::Now.ToString('yyyy'))</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <InvariantGlobalization>true</InvariantGlobalization>
