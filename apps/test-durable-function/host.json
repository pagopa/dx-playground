{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "durableTask": {
      "storageProvider": {
        "connectionStringName": "DurableFn"
      },
      "hubName": "%SLOT_TASK_HUBNAME%",
      "useGracefulShutdown": true
    }
  }
}
