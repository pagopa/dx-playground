{
  "name": "dx-playground",
  "private": true,
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538",
  "scripts": {
    "code-review": "turbo run typecheck lint:check test",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "version": "yarn changeset version && yarn --mode update-lockfile",
    "version:snapshot": "yarn changeset version --snapshot rc && yarn --mode update-lockfile",
    "release": "yarn build && changeset publish",
    "release:snapshot": "yarn build && changeset publish --no-git-tag --tag rc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@vitest/coverage-v8": "^3.1.3",
    "turbo": "^2.5.3",
    "vitest": "^3.1.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "infra/*"
  ]
}
